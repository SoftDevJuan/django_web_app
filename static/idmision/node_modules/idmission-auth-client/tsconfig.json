{
  "extends": "./tsconfig.base.json",
  "exclude": ["node_modules", "src/stories"],
  "include": ["scripts", "src", "test"],
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationDir": null,
    "module": "commonjs",
    "target": "es2015",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
